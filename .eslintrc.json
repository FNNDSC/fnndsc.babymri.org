{
  "extends": ["eslint:recommended", "google"],
  "env": {
    "browser": true
  },
  "plugins": [
    "html"
  ],
  "rules": {
    "no-var": "off",
    "new-cap": ["error", { "capIsNewExceptions": ["Polymer"] }],
    "keyword-spacing": "error",
    "max-len": "warn"
  },
  "globals": {
    "Polymer": true,
    "SectionBehavior": true,
    "MembersBehavior": true,
    "ContactsBehavior": true,
    "TOCBehavior": true,
    "PublicationsBehavior": true
  }
}
